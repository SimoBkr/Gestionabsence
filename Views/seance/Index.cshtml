@model IEnumerable<coreProject.Models.Seance>
<!DOCTYPE html>
<html lang="en">
<head>
       <meta charset="UTF-8">
       <meta name="viewport" content="width=device-width, initial-scale=1.0">
       <link href="~/dist/css/index.css" type="stylesheet">
       <title>Document</title>
       <style>
              body{
                     width: 100%;
                     height: 100%;
              }
       </style>
</head>
<body>
       <br><br>
       <a asp-action="Create" class="btn btn-primary">Create</a>
  <div class="panel panel-default">
         <br>
   <div class="panel-heading">
      Liste des seances
   </div>
   <br>
   <div class="panel-body">
    <table class="table table-bordred">
           <thead>
           <tr>
                     <td>code Seance</td>
                     <td>code Matiere</td>
                     <td>code Prof</td>
                     <td>code Filiere</td>
                     <td>Methods</td>
                     
           </tr>
           </thead>
         <tbody>
           @foreach (var item in Model)
           {
               <tr>
                     <td>@Html.DisplayFor(ModelItem => item.codeSeance)</td>
                     <td>@Html.DisplayFor(ModelItem => item.codeMatiere)</td>
                     <td>@Html.DisplayFor(ModelItem => item.codeProf)</td>
                     <td>@Html.DisplayFor(ModelItem => item.codeFiliere)</td>
                     
                     <td>
                          <button class="btn btn-primary">@Html.ActionLink("Details","Details",new {item.codeSeance})</button>
                         <button class="btn btn-success">@Html.ActionLink("Edit","Edit",new {item.codeSeance})</button>
                          <button class="btn btn-danger">@Html.ActionLink("Delete","Delete",new {item.codeSeance})</button>
                     </td>
           </tr>
           }
            </tbody>
    </table>
     </div>
   
    </div>
</body>
</html>    
 