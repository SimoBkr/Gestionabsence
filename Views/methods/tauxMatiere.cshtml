@model IEnumerable<coreProject.Models.tauxMatiere>
  <!DOCTYPE html>
  <html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/dist/css/index.css" type="stylesheet">
    <title>Document</title>
  </head>
  <body>
    <br><br>
    <div class="panel panel-default">
      <br>
   <div class="panel-heading">
      Taux d'absenteismes par matiere
   </div>
   <br>
   <div class="panel-body">
<table class="table table-bordred">
  <thead>
  <tr>
         <td style="background: red;">nom Matiere</td>
         <td>Taux</td>
         
  </tr>
  </thead>
         <tbody>
  @foreach (var item in Model)
  {
      <tr>
          <td>@Html.DisplayFor(ModelItem => item.nomMatiere)</td>
          <td>@Html.DisplayFor(ModelItem => item.taux)</td>
          
  </tr>
  }
  </tbody>
</table>
</div>
   
  </div>

  </body>
  </html>
  